{
  "name": "drawit",
  "version": "0.0.0",
  "description": "A graph library based on d3 for simple manipulation with customization.",
  "main": "lib/drawit.js",
  "scripts": {
    "build": "babel src -d lib && ./node_modules/uglify-js/bin/uglifyjs lib/drawit.js --compress -o lib/drawit.min.js",
    "jshint": "./node_modules/jshint/bin/jshint src/*js || true",
    "clean": "rm -rf ./lib/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepakshrma/drawit.git"
  },
  "keywords": [
    "d3",
    "drawit"
  ],
  "author": "Deepak Vishwakarma <deepak.m.shrma@gmail.com> (https://github.com/deepakshrma)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deepakshrma/drawit/issues"
  },
  "homepage": "https://github.com/deepakshrma/drawit#readme",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "jshint": "^2.9.2",
    "uglify-js": "^2.6.2"
  }
}
